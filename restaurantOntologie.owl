<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:schema="https://schema.org/">

  <!-- Classe Restaurant -->
  <rdf:Description rdf:about="https://schema.org/Restaurant">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Restaurant</rdfs:label>
  </rdf:Description>

  <!-- Classe Menu -->
  <rdf:Description rdf:about="https://schema.org/Menu">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Menu</rdfs:label>
  </rdf:Description>

  <!-- Classe MenuItem -->
  <rdf:Description rdf:about="https://schema.org/MenuItem">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>MenuItem</rdfs:label>
  </rdf:Description>

  <!-- Classe Review -->
  <rdf:Description rdf:about="https://schema.org/Review">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Review</rdfs:label>
  </rdf:Description>

  <!-- Classe Location -->
  <rdf:Description rdf:about="https://schema.org/Place">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Place</rdfs:label>
  </rdf:Description>

  <!-- Propriété menu -->
  <rdf:Property rdf:about="https://schema.org/menu">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="https://schema.org/Menu"/>
  </rdf:Property>

  <!-- Propriété menuItem -->
  <rdf:Property rdf:about="https://schema.org/menuItem">
    <rdfs:domain rdf:resource="https://schema.org/Menu"/>
    <rdfs:range rdf:resource="https://schema.org/MenuItem"/>
  </rdf:Property>

  <!-- Propriété review -->
  <rdf:Property rdf:about="https://schema.org/review">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="https://schema.org/Review"/>
  </rdf:Property>

  <!-- Propriété location -->
  <rdf:Property rdf:about="https://schema.org/location">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="https://schema.org/Place"/>
  </rdf:Property>

  <!-- Propriété offersDelivery -->
  <rdf:Property rdf:about="https://schema.org/offersDelivery">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Property>

  <!-- Propriété offersTakeout -->
  <rdf:Property rdf:about="https://schema.org/offersTakeout">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Property>

  <!-- Propriété dietaryRestriction -->
  <rdf:Property rdf:about="https://schema.org/dietaryRestriction">
    <rdfs:domain rdf:resource="https://schema.org/MenuItem"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Propriété calorieContent -->
  <rdf:Property rdf:about="https://schema.org/calorieContent">
    <rdfs:domain rdf:resource="https://schema.org/MenuItem"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Propriété paymentAccepted -->
  <rdf:Property rdf:about="https://schema.org/paymentAccepted">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

</rdf:RDF>
