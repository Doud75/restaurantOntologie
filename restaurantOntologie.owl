<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:schema="https://schema.org/">

  <!-- Classe Restaurant -->
  <rdf:Description rdf:about="https://schema.org/Restaurant">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Restaurant</rdfs:label>
  </rdf:Description>

  <!-- Classe AggregateRating -->
  <rdf:Description rdf:about="https://schema.org/AggregateRating">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>AggregateRating</rdfs:label>
  </rdf:Description>

  <!-- Classe PostalAddress -->
  <rdf:Description rdf:about="https://schema.org/PostalAddress">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>PostalAddress</rdfs:label>
  </rdf:Description>

  <!-- Classe OpeningHoursSpecification -->
  <rdf:Description rdf:about="https://schema.org/OpeningHoursSpecification">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>OpeningHoursSpecification</rdfs:label>
  </rdf:Description>

  <!-- Propriété name -->
  <rdf:Property rdf:about="https://schema.org/name">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Propriété aggregateRating -->
  <rdf:Property rdf:about="https://schema.org/aggregateRating">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="https://schema.org/AggregateRating"/>
  </rdf:Property>

  <!-- Propriété address -->
  <rdf:Property rdf:about="https://schema.org/address">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="https://schema.org/PostalAddress"/>
  </rdf:Property>

  <!-- Propriété telephone -->
  <rdf:Property rdf:about="https://schema.org/telephone">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Propriété url -->
  <rdf:Property rdf:about="https://schema.org/url">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </rdf:Property>

  <!-- Propriété openingHoursSpecification -->
  <rdf:Property rdf:about="https://schema.org/openingHoursSpecification">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="https://schema.org/OpeningHoursSpecification"/>
  </rdf:Property>

  <!-- Propriété servesCuisine -->
  <rdf:Property rdf:about="https://schema.org/servesCuisine">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Propriété priceRange -->
  <rdf:Property rdf:about="https://schema.org/priceRange">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Propriété takesReservations -->
  <rdf:Property rdf:about="https://schema.org/takesReservations">
    <rdfs:domain rdf:resource="https://schema.org/Restaurant"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Property>

</rdf:RDF>

