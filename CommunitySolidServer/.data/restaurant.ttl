@prefix : <http://www.example.org/restaurant-ontology#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Restaurant 1
<https://example.org/restaurant1> a :Restaurant ;
    :hasName "Le Jardin Secret"^^xsd:string ;
    :hasRating [
        a :AggregateRating ;
        :ratingValue "4.6"^^xsd:decimal ;
        :reviewCount "180"^^xsd:integer
    ] ;
    :hasAddress [
        a :Address ;
        :streetAddress "22 Rue des Fleurs"^^xsd:string ;
        :addressLocality "Lyon"^^xsd:string ;
        :addressRegion "Auvergne-Rhône-Alpes"^^xsd:string ;
        :postalCode "69001"^^xsd:string
    ] ;
    :hasPhone "+33 4 72 34 56 78"^^xsd:string ;
    :hasWebsite <http://www.lejardinsecret.fr> ;
    :hasOpeningHours [
        a :OpeningHours ;
        :dayOfWeek :Monday, :Tuesday, :Wednesday, :Thursday, :Friday ;
        :opens "11:00"^^xsd:time ;
        :closes "23:00"^^xsd:time
    ], [
        a :OpeningHours ;
        :dayOfWeek :Saturday ;
        :opens "12:00"^^xsd:time ;
        :closes "23:30"^^xsd:time
    ] ;
    :servesCuisine "Française"^^xsd:string ;
    :priceRange "$$$"^^xsd:string ;
    :acceptsReservations true .

